{
	"name": "fastify-floc-off",
	"version": "4.0.1",
	"description": "Fastify plugin to opt out of Google's Topics advertising-surveillance API",
	"keywords": [
		"block",
		"fastify",
		"floc",
		"google",
		"headers",
		"plugin",
		"opt out",
		"opt-out",
		"topics",
		"tracking"
	],
	"main": "src/index.js",
	"type": "commonjs",
	"types": "types/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Fdawgs/fastify-floc-off.git"
	},
	"homepage": "https://github.com/Fdawgs/fastify-floc-off",
	"bugs": {
		"url": "https://github.com/Fdawgs/fastify-floc-off/issues"
	},
	"license": "MIT",
	"author": "Frazer Smith <frazer.dev@icloud.com>",
	"funding": "https://github.com/sponsors/Fdawgs",
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"lint:eslint:fix": "npm run lint:eslint -- --fix",
		"lint:eslint": "eslint . --cache",
		"lint:fix": "npm run lint:eslint:fix && npm run lint:prettier:fix",
		"lint:licenses": "licensee --errors-only --production",
		"lint:prettier:fix": "prettier . -w -u",
		"lint:prettier": "prettier . -c -u",
		"lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:licenses",
		"prepare": "husky",
		"test:unit:coverage": "c8 --100 --all --src='src/' -r=lcovonly -r=text npm run test:unit",
		"test:unit": "node --test",
		"test": "npm run lint && npm run lint:prettier && npm run test:unit",
		"typecheck": "tsc"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^20.1.0",
		"@commitlint/config-conventional": "^20.0.0",
		"@eslint/compat": "^1.4.0",
		"@fdawgs/eslint-config": "^1.0.9",
		"@types/node": "^24.7.0",
		"c8": "^10.1.3",
		"eslint": "^9.37.0",
		"fastify": "^5.6.1",
		"husky": "^9.1.7",
		"licensee": "^11.1.1",
		"prettier": "^3.6.2",
		"typescript": "~5.9.3"
	},
	"dependencies": {
		"fastify-plugin": "^5.1.0"
	}
}
